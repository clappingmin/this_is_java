package chap03;

public class AppleBox {

	public static void main(String[] args) {

		int apple = 200;
		int perBox = 12;

		int box = apple / perBox;
		int remainder = apple % perBox;

		System.out.println("사과의 총 개수 : " + apple);
		System.out.println("box의 개수 : " + box);
		System.out.println("남은 사과의 개수 : " + remainder);

	}

}
